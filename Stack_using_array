#include <stdio.h>
int top = -1;
int MAX = 5;

void push (int a[],int data);
void pop();

int main()
{ 
    
    int a[MAX];
    
   push(a,4);
   push(a,7);
   push(a,2);
    push(a,4);
  //push(a,7);
   //push(a,9);
   
   pop();
//   pop();
//   pop();
//   pop();
  
   
  
   
    printf("Hello World\n");
      for(int i=0;i<top+1 && i<MAX;i++)
    {
        printf("%d " ,a[i]);
    }

    return 0;
}

void push (int a[],int data)
{
   
    
    if(top <MAX-1)
    {
        top++;
    a[top] = data; 
   
    }else{
        printf("Stack Overflow\n");
        top = MAX+1;
    }
   
    
    
}

void pop()
{
    if(top > -1)
    {
       top--; 
    }
    else{
        
        printf("Stack is EMPTY\n");
        top = -1;
    }
    
    
    
}



