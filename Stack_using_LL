#include <stdio.h>
#include <stdlib.h>


typedef struct node
{
    int data;
    struct node *link;
    
}node;
 
 node *top =0;
 

void push(int data);

void pop(void);

int main()
{
    
   
    push(1);
    push(2);
    pop();
   
   node *ptr = top;
   while(ptr)
   {
       printf("%d ",ptr->data);
       ptr = ptr->link;
       
   }

    return 0;

    
}                                                                                                      


void push(int data)
{
    
   
   node *temp = malloc(sizeof(node));
   temp->data = data;
   temp->link = NULL;
   
   temp->link = top;
   top = temp;
   
   
   
    
    
}

void pop(void)
{
    node *temp = top;
    top = top->link;
    free(temp);
    
    
    
    
}




